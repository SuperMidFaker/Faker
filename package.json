{
  "name": "welogix-web",
  "version": "0.0.1",
  "description": "welogix cooperation web system",
  "main": "server/sboot.js",
  "scripts": {
    "clean": "rm -rf public/dist",
    "debug": "node --debug .",
    "lint": "eslint --ext .js --ext .jsx client server universal webpack",
    "hot": "webpack-dev-server --config webpack/dev.js",
    "build-dev": "NODE_ENV=production webpack --progress --profile --diplay-modules --display-origins --config webpack/dev.js",
    "dev": "nodemon",
    "start": "npm run clean & npm run hot &  npm run dev",
    "test-build": "NODE_ENV=test webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "test-prod": "NODE_ENV=test node . --port=3022",
    "test-deploy": "npm run clean && npm run test-build && npm run test-prod",
    "build": "NODE_ENV=production webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "prod": "NODE_ENV=production node . --port=3022",
    "pm2": "pm2 start pm2/prod.js",
    "deploy": "npm run clean && npm run build && npm run pm2",
    "test": "mocha --reporter spec --check-leaks tests/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.welogix.cn:saas/welogix-web.git"
  },
  "keywords": [
    "tms",
    "wms",
    "collaboration",
    "sass",
    "react",
    "router",
    "koa"
  ],
  "author": "The Welogix Team",
  "license": "MIT",
  "dependencies": {
    "ant-ui": "^0.11.8",
    "async-validator": "^1.3.5",
    "babel": "^5.8.29",
    "bcrypt": "^0.8.5",
    "co-body": "^4.0.0",
    "co-busboy": "^1.3.1",
    "generic-pool": "^2.2.0",
    "history": "^1.17.0",
    "json-loader": "^0.5.4",
    "koa": "^1.1.1",
    "koa-jwt": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.2.3",
    "koa-static": "^1.5.1",
    "moment": "^2.10.6",
    "mysql": "^2.9.0",
    "query-string": "^3.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-dropzone": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redbox-react": "^1.1.1",
    "redux": "^3.0.4",
    "serialize-javascript": "^1.1.2",
    "superagent": "^1.4.0",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.26"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^5.8.35",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.35",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.4",
    "postcss-loader": "^0.8.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
