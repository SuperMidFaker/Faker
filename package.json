{
  "name": "welogix-web",
  "version": "2.2.0",
  "title": "WeLogix Web",
  "description": "A SaaS platform for Cross-border Supply Chain and Logistics",
  "homepage": "https://welogix.cn/",
  "keywords": [
    "TMS",
    "CMS",
    "WMS",
    "logistics",
    "SaaS",
    "react",
    "router",
    "koa"
  ],
  "author": "WeLogix InfoTech",
  "main": "server/sboot.js",
  "scripts": {
    "clean": "rm -rf public/dist",
    "debug": "node --debug .",
    "lint": "eslint --fix --ext .js --ext .jsx client server common",
    "lint-staged": "lint-staged",
    "hot": "webpack-dev-server --config webpack/dev.js",
    "dev": "nodemon",
    "start": "npm run clean & npm run hot &  npm run dev",
    "test-build": "npm run build-i18n && NODE_ENV=test node --max_old_space_size=4096 ./node_modules/.bin/webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "test-prod": "NODE_ENV=test node . --port=3022",
    "test-deploy": "npm run clean && npm run test-build && npm run test-prod",
    "build": "npm run build-i18n & NODE_ENV=production node --max_old_space_size=4096 ./node_modules/.bin/webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "build-staging": "npm run build-i18n & NODE_ENV=staging node --max_old_space_size=8192 ./node_modules/.bin/webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "build-i18n": "BABEL_ENV=i18n babel client common -d public/dist/messages && babel-node scripts/translation/extract",
    "prod": "NODE_ENV=production node . --port=3022",
    "pm2": "pm2 start pm2/prod.json",
    "deploy": "npm run clean && npm run build && npm run pm2",
    "test": "mocha --reporter spec --bail --check-leaks tests/*_test.js",
    "jest": "jest --coverage --config .jest.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@git.welogix.cn:saas/welogix-web.git"
  },
  "license": "MIT",
  "pre-commit": "lint-staged",
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "antd": "~3.4.5",
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015-without-regenerator": "^1.0.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bizcharts": "^3.1.3",
    "braft-editor": "^1.1.14",
    "browser-audio": "^1.0.2",
    "classnames": "^2.2.5",
    "codemirror": "^5.32.0",
    "css-loader": "^0.28.1",
    "currency-formatter": "^1.2.1",
    "detect-browser": "2.0.0",
    "echarts": "^3.8.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "file-saver": "^1.3.8",
    "glob": "^7.1.1",
    "happypack": "^4.0.1",
    "immutability-helper": "^2.6.0",
    "intl": "^1.2.5",
    "jsbarcode": "^3.8.0",
    "json-loader": "^0.5.4",
    "koa": "^1.2.4",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash-decorators": "^4.4.1",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.10",
    "rc-queue-anim": "^1.4.1",
    "rc-tween-one": "^1.3.1",
    "react": "^16.3.1",
    "react-codemirror2": "^3.0.7",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^16.3.1",
    "react-fittext": "^1.0.0",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.4",
    "react-resizable": "^1.7.5",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.19.1",
    "superagent": "^3.8.2",
    "url-loader": "^0.6.2",
    "warning": "^3.0.0",
    "webpack": "^1.15.0",
    "webpack-isomorphic-tools": "^2.6.4",
    "xlsx": "^0.11.13"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.12.7",
    "pre-commit": "^1.1.3",
    "webpack-dev-server": "^1.16.5"
  }
}
