{
  "name": "welogix-web",
  "version": "0.0.1",
  "description": "welogix cooperation web system",
  "main": "server/sboot.js",
  "scripts": {
    "clean": "rm -rf public/dist",
    "debug": "node --debug .",
    "lint": "eslint --ext .js --ext .jsx client server universal webpack",
    "hot": "npm run build-i18n & webpack-dev-server --config webpack/dev.js",
    "build-dev": "NODE_ENV=production webpack --progress --profile --diplay-modules --display-origins --config webpack/dev.js",
    "dev": "nodemon",
    "start": "npm run clean & npm run hot &  npm run dev",
    "test-build": "NODE_ENV=test webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "test-prod": "NODE_ENV=test node . --port=3022",
    "test-deploy": "npm run clean && npm run test-build && npm run test-prod",
    "build": "npm run build-i18n & NODE_ENV=production webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "build-i18n": "babel --babelrc .i18n.babelrc client reusable > /dev/null && babel-node scripts/translation/extract",
    "build-i18n2": "babel --babelrc .i18n.babelrc .> /dev/null && babel-node scripts/translation/extract",
    "prod": "NODE_ENV=production node . --port=3022",
    "pm2": "pm2 start pm2/prod.js",
    "deploy": "npm run clean && npm run build && npm run pm2",
    "test": "mocha --reporter spec --check-leaks tests/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.welogix.cn:saas/welogix-web.git"
  },
  "keywords": [
    "tms",
    "wms",
    "collaboration",
    "sass",
    "react",
    "router",
    "koa"
  ],
  "author": "The Welogix Team",
  "license": "MIT",
  "pre-commit": [],
  "dependencies": {
    "ant-ui": "^0.12.1",
    "babel": "^5.8.35",
    "bcrypt": "^0.8.5",
    "co-body": "^4.0.0",
    "co-busboy": "^1.3.1",
    "generic-pool": "^2.4.1",
    "glob": "^7.0.0",
    "history": "^1.17.0",
    "intl": "^1.1.0",
    "koa": "^1.1.1",
    "koa-jwt": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^1.5.1",
    "moment": "^2.11.2",
    "mysql": "^2.9.0",
    "query-string": "^3.0.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-dropzone": "^3.0.0",
    "react-intl": "^2.0.0-beta-2",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "redux": "^3.3.1",
    "serialize-javascript": "^1.2.0",
    "superagent": "^1.4.0",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.30"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^5.8.35",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-intl": "^1.0.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.35",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.4",
    "postcss-loader": "^0.8.1",
    "pre-commit": "^1.1.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
