{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": './webpack/wpbase.js'
      }
    }
  },
  "globals": {
    "__CLIENT__": true,
    "__CDN__": true,
    "webpackIsomorphicTools": false,
    "__PRODUCTIONS_ROOT_GROUP__": false,
    "__PRODUCTIONS_DOMAIN_GROUP__": false,
    "__DEV__": false,
    "__PROD__": false,
    "__DEVTOOLS__": false,
    "__API_ROOT__": false
  },
  "rules": {
    "comma-dangle": [0, "never"],
    "key-spacing": [0, {
      "beforeColon": true,
      "afterColon": true
    }],
    "no-else-return": 0,
    "react/jsx-quotes": [0, "single"],
    "object-curly-spacing": 0,
    "array-bracket-spacing": 0,
    "indent": 0,
    "react/jsx-first-prop-new-line": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "react/jsx-space-before-closing": 0,
    "react/jsx-curly-spacing": 0,
    "react/jsx-indent": 0,
    "react/prefer-stateless-function": 0,
    "block-spacing": 0,
    "no-restricted-syntax": 0,
    "space-before-function-paren": [0, "always"],
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-indent-props": 0,
    "id-length":[0],
    "max-len": 0,
    "quote-props": 0,
    "arrow-body-style": 0,
    "react/jsx-no-bind": 0,
    "react/prop-types": 0,
    "consistent-return": 0
  }
}
