{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": './webpack/wpbase.js'
      }
    }
  },
  "globals": {
    "__CLIENT__": true,
    "__CDN__": true,
    "webpackIsomorphicTools": false,
    "__PRODUCTIONS_ROOT_GROUP__": false,
    "__PRODUCTIONS_DOMAIN_GROUP__": false,
    "__DEV__": false,
    "__PROD__": false,
    "__DEVTOOLS__": false,
    "API_ROOTS": false
  },
  "rules": {
    "no-else-return": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "no-restricted-syntax": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-indent": 0,
    "id-length":[0],
    "max-len": 0,
    "indent": 0,
    "react/jsx-no-bind": 0,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "consistent-return": 0
  }
}
